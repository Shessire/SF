@model SF.Models.WITPercent

@{
    ViewData["Title"] = "Delete WIT Percentage";
}

<div class="container mt-4">
    <h2>Delete WIT Percentage</h2>
    <h3>Are you sure you want to delete this WIT Percentage?</h3>

    <div class="my-3">
        <dl class="row">
            <dt class="col-sm-3">Value</dt>
            <dd class="col-sm-9">@Model.Value</dd>
        </dl>
    </div>

    <form asp-action="Delete" asp-route-id="@Model.Id" method="post">
        <button type="submit" class="btn btn-danger">Delete</button>
        <a asp-action="Index" class="btn btn-secondary">Cancel</a>
    </form>
</div>
